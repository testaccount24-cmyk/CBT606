//RHPA JOB X,PETTY,CLASS=1,MSGCLASS=X
//*-----------------------------------------------------------------*
//*   ASSEMBLE ALL MODULES AND LINKEDIT THEM TOGETHER
//*-----------------------------------------------------------------*
//ASMIT  PROC  MEMB=XXXX,LIBR='RHP.A',
//             OBJLIB='RHP.O',
//             MAC1='SYS1.MACLIB',
//             MAC2='SYS1.MACLIB' (SYS1.AMODGEN ON SOME SYSTEMS)
//*
//*     ASM LINKEDIT FROM SOURCE ASM
//*
//ASMH EXEC PGM=ASMA90,PARM=(OBJECT,NODECK,NOESD,NORLD),COND=(4,LT)
//SYSLIB   DD DISP=SHR,DSN=&MAC1
//         DD DISP=SHR,DSN=&MAC2
//         DD DISP=SHR,DSN=&LIBR
//SYSIN    DD DISP=SHR,DSN=&LIBR.(&MEMB)
//SYSUT1   DD UNIT=SYSDA,SPACE=(CYL,4)
//SYSPRINT DD SYSOUT=*
//SYSLIN   DD DISP=SHR,DSN=&OBJLIB.(&MEMB)
//   PEND
//*
//AWSSL    EXEC ASMIT,MEMB=AWSSL
//ADLER32  EXEC ASMIT,MEMB=ADLER32
//BLKSRT   EXEC ASMIT,MEMB=BLKSRT
//BZINTERR EXEC ASMIT,MEMB=BZINTERR
//BZLIB    EXEC ASMIT,MEMB=BZLIB
//COMPRESB EXEC ASMIT,MEMB=COMPRESB
//COMPRESS EXEC ASMIT,MEMB=COMPRESS
//CRCTABLE EXEC ASMIT,MEMB=CRCTABLE
//CRC32    EXEC ASMIT,MEMB=CRC32
//DECOMPR  EXEC ASMIT,MEMB=DECOMPR
//DEFLATE  EXEC ASMIT,MEMB=DEFLATE
//HUFFMAN  EXEC ASMIT,MEMB=HUFFMAN
//INFBLOCK EXEC ASMIT,MEMB=INFBLOCK
//INFCODES EXEC ASMIT,MEMB=INFCODES
//INFFAST  EXEC ASMIT,MEMB=INFFAST
//INFLATE  EXEC ASMIT,MEMB=INFLATE
//INFTREES EXEC ASMIT,MEMB=INFTREES
//INFUTIL  EXEC ASMIT,MEMB=INFUTIL
//LIB      EXEC ASMIT,MEMB=LIB
//MEMPOOL  EXEC ASMIT,MEMB=MEMPOOL
//PRINTF   EXEC ASMIT,MEMB=PRINTF
//PROFILE  EXEC ASMIT,MEMB=PROFILE
//RANDTAB  EXEC ASMIT,MEMB=RANDTAB
//STACK    EXEC ASMIT,MEMB=STACK
//TREES    EXEC ASMIT,MEMB=TREES
//UNCOMPR  EXEC ASMIT,MEMB=UNCOMPR
//ZUTIL    EXEC ASMIT,MEMB=ZUTIL
//*
//LKED   EXEC  PGM=IEWL,COND=(4,LT),
//       PARM='MAP,LIST,LET'
//SYSUT1   DD  UNIT=SYSDA,SPACE=(2048,(200,20))
//SYSPRINT DD  SYSOUT=*
//SYSLIB   DD  DISP=SHR,DSN=CEE.SCEELKED
//SYSLMOD  DD  DISP=SHR,DSN=(YOUR AUTHORIZED LIBRARY)
//OBJECT   DD  DISP=SHR,DSN=RHP.O(AWSSL)
//         DD  DISP=SHR,DSN=RHP.O(ADLER32)
//         DD  DISP=SHR,DSN=RHP.O(BLKSRT)
//         DD  DISP=SHR,DSN=RHP.O(BZINTERR)
//         DD  DISP=SHR,DSN=RHP.O(BZLIB)
//         DD  DISP=SHR,DSN=RHP.O(COMPRESB)
//         DD  DISP=SHR,DSN=RHP.O(COMPRESS)
//         DD  DISP=SHR,DSN=RHP.O(CRCTABLE)
//         DD  DISP=SHR,DSN=RHP.O(CRC32)
//         DD  DISP=SHR,DSN=RHP.O(DECOMPR)
//         DD  DISP=SHR,DSN=RHP.O(DEFLATE)
//         DD  DISP=SHR,DSN=RHP.O(HUFFMAN)
//         DD  DISP=SHR,DSN=RHP.O(INFBLOCK)
//         DD  DISP=SHR,DSN=RHP.O(INFCODES)
//         DD  DISP=SHR,DSN=RHP.O(INFFAST)
//         DD  DISP=SHR,DSN=RHP.O(INFLATE)
//         DD  DISP=SHR,DSN=RHP.O(INFTREES)
//         DD  DISP=SHR,DSN=RHP.O(INFUTIL)
//         DD  DISP=SHR,DSN=RHP.O(LIB)
//         DD  DISP=SHR,DSN=RHP.O(MEMPOOL)
//         DD  DISP=SHR,DSN=RHP.O(PRINTF)
//         DD  DISP=SHR,DSN=RHP.O(PROFILE)
//         DD  DISP=SHR,DSN=RHP.O(RANDTAB)
//         DD  DISP=SHR,DSN=RHP.O(STACK)
//         DD  DISP=SHR,DSN=RHP.O(TREES)
//         DD  DISP=SHR,DSN=RHP.O(UNCOMPR)
//         DD  DISP=SHR,DSN=RHP.O(ZUTIL)
//SYSLIN   DD  *
 INCLUDE OBJECT
 SETCODE AC(1)
 ENTRY   AWSSL
 NAME    AWSSL(R)
